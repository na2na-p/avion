{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"fmt": {
			"dependsOn": ["^fmt"]
		},
		"test": {
			"dependsOn": ["^test"]
		},
		"type-check": {
			"dependsOn": ["^type-check"]
		}
	}
}
